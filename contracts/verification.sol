pragma ton-solidity >=0.30.0;
pragma AbiHeader pubkey;

contract Verify {
    bytes constant m_vkey = hex"";

    function verify(bytes proof) public view returns (bool) {
        tvm.accept();
        string blob_str = proof;
        blob_str.append(m_vkey);
        return tvm.vergrth16(blob_str);
    }
}
